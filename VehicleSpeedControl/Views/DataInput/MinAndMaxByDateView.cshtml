@model IEnumerable<VehicleSpeedControl.Models.Record>

@{
    ViewBag.Title = "Результаты запроса по минимальной и максимальной скорости";
}

    <h2>Результаты запроса по минимальной и максимальной скорости за указанную дату</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RegistrationTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VehicleLicensePlate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VehicleSpeed)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RegistrationTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VehicleLicensePlate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VehicleSpeed)
            </td>
        </tr>
    }

</table>

<div class="footerLinks">
    @Html.ActionLink("К списку записей", "Index")
    @Html.ActionLink("На главную", "Index", "Home")
</div>
